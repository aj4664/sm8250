# Local Version
CONFIG_LOCALVERSION="-MagicTime"
CONFIG_LOCALVERSION_AUTO=y

# PELT
CONFIG_PELT_COMPATIBILITY_LAYER=y
CONFIG_SCHED_THERMAL_PRESSURE=y
CONFIG_UCLAMP_TASK=y
CONFIG_UCLAMP_ASSIST=y
CONFIG_UCLAMP_TASK_GROUP=y
CONFIG_UCLAMP_BUCKETS_COUNT=20
CONFIG_SCHED_CASS=y
CONFIG_JUMP_LABEL=y

# LRNG
CONFIG_LRNG=y

# ZRAM
CONFIG_ZRAM_BACKEND_LZ4=y
CONFIG_ZRAM_BACKEND_LZO=n

# KernelSU
CONFIG_KSU_SUSFS_SUS_OVERLAYFS=y

# TEO
CONFIG_CPU_IDLE_GOV_TEO=y

# Warn
CONFIG_FRAME_WARN=4096

# CPUSET
CONFIG_CPUSET_ASSIST=y
CONFIG_CPUSET_AUDIO_APP="0-3"
CONFIG_CPUSET_FG="0-3,5-6"
CONFIG_CPUSET_RESTRICTED="0-3"
CONFIG_CPUSET_DISPLAY="0-7"

# ION
CONFIG_ION_SYSTEM_HEAP=y
